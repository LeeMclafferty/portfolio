.centered
{
 text-align: center;
}
.large
{
 font-size: large;
}
.medium
{
 font-size: medium;
}
.small
{
 font-size: small;
}
.bold
{
	font-weight: bold;
}
.toupper
{
	text-transform: uppercase;
}
.navitem
{
	display: inline-block;
	text-align: center;
	font-size: 16px;
	line-height: 1.5em;
	font-family: proxima-nova;
    font-weight: 400;
    font-style: normal;
    text-transform: uppercase;
    letter-spacing: 2px;
		margin: 0 auto 0px;
		margin-top: auto;
		margin-right: 10px;
		margin-bottom: auto;
		margin-left: 10px;
}	
.navblock
{
	width: auto;
    text-align: center;
    border-bottom: 1px solid rgba(224,224,224,0);
    border-top: 1px solid rgba(224,224,224,0);
    margin: 0;
    padding: 28px 0;
}
.body
{
	color: rgb(196, 191, 181);
	font-size: 15px;
	font-family: proxima-nova, Arial Narrow;
	font-weight: lighter;
	text-align: center;
	margin: 5px;
}
.pagetitle
{
	text-align: center;
	color: rgb(242, 243, 245);
	display: block;
	text-transform: uppercase;
	font-family: proxima-nova, Arial Narrow;
	font-size: 50px;
	font-weight: 900;
	margin: 0 auto 28px;
		margin-top: 28px;
		margin-right: auto;
		margin-bottom: 5px;
		margin-left: auto;
}
.subheader
{
	text-align: center;
	color: rgb(242, 243, 245);
	display: inline-block;
	text-transform: uppercase;
	font-family: proxima-nova, Arial Narrow;
	font-size: 25px;
	font-weight: light;
	margin: 0 auto 28px;
		margin-top: 0px;
		margin-right: auto;
		margin-bottom: 5px;
		margin-left: auto;
}
.paragraph
{
	white-space: pre-wrap;
	word-wrap: break-word;
	display: block;
	font-family: proxima-nova, Times New Roman;
	color:rgb(196, 191, 181);
	font-weight: 400;
	font-size: 16px;
	font-style: normal;
	text-align: left;
		margin-top: 0px;
		margin-right: 350px;
		margin-bottom: 0px;
		margin-left: 350px;
	line-height: 1.7em;
}